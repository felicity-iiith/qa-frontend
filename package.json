    "markdown-it-katex": "^2.0.3",
    "milligram": "^1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.7.1",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
